{
  "expo": {
    "name": "Flick",
    "slug": "Flick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flick-logo-sq-zo.png",
    "scheme": "templateexpobasic",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Flick"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flick-logo-sq-zo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Flick",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/flick-logo-sq-zo.png"
          },
          "image": "./assets/images/flick-logo-sq-zo.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-web-browser",
      "expo-font",
      "expo-media-library"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c94f5e96-2411-4010-b470-14068175051d"
      }
    }
  }
}